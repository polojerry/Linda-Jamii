<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.initialvisit.physicalAntenatalFeedingProfile.PhysicalAntenatalFeeding">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_physical_examination"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/text_label_physical_examination"
                        style="@style/Theme.LindaJamii.TextVisitHeading"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="4dp"
                        android:text="Physical Examination"
                        android:textAlignment="textStart"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_general"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_label_physical_examination">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_general"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="General"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_bp"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="4dp"
                        app:layout_constraintEnd_toStartOf="@+id/text_layout_physical_examination_height"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_general">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_bp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="BP"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_height"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/text_layout_physical_examination_bp"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_general">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_height"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Heignt"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_cvs"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="4dp"
                        app:layout_constraintEnd_toStartOf="@+id/text_layout_physical_examination_Resp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_bp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_cvs"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="CVS"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_Resp"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/text_layout_physical_examination_cvs"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_height">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_resp"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Resp"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_breast"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="4dp"
                        app:layout_constraintEnd_toStartOf="@+id/text_layout_physical_examination_abdomen"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_cvs">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_breast"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Breast"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_abdomen"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/text_layout_physical_examination_breast"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_Resp">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_abdomen"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Abdomen"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_vaginal_examination"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="4dp"
                        app:layout_constraintEnd_toStartOf="@+id/text_layout_physical_examination_discharge_genital_ulcer"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_breast">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_vaginal_examination"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="VaginalExamination"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_physical_examination_discharge_genital_ulcer"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/text_layout_physical_examination_vaginal_examination"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_physical_examination_abdomen">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_pysical_examination_discharge_genital_ulcer"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Discharge/ Genital Ulcer"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_antenatal_profile"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_physical_examination">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/text_label_antenatal_profile"
                        style="@style/Theme.LindaJamii.TextVisitHeading"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:text="Antenatal Profile"
                        android:textAlignment="textStart"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_hb"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_label_antenatal_profile">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_hb"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="HB"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_blood_group"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="4dp"
                        app:layout_constraintEnd_toStartOf="@+id/text_layout_antenatal_profile_rhesus_factor"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_antenatal_profile_hb">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_blood_group"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Blood Group"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_rhesus_factor"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/text_layout_antenatal_profile_blood_group"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_antenatal_profile_hb">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_rhesus_factor"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Rhesus Fact"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_serelogy"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_antenatal_profile_blood_group">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_serelogy"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Serelogy (VDRL/ RPR)"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_tb_screning"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_antenatal_profile_serelogy">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_tb_screeening"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="TB Screening as per the intensive case finding tool"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_hiv"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="4dp"
                        app:layout_constraintEnd_toStartOf="@+id/text_layout_antenatal_profile_urinalisis"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_antenatal_profile_tb_screning">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_hiv"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="HIV"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_urinalisis"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="4dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/text_layout_antenatal_profile_hiv"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_antenatal_profile_tb_screning">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_urinalisis"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Urinalysis"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_antenatal_profile_hiv_couseling_and_testing"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_antenatal_profile_hiv">

                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/text_antenatal_profile_hiv_couseling_and_testing"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Couple HIV Counselling and testing Done"
                            android:inputType="text" />

                    </com.google.android.material.textfield.TextInputLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/layout_infant_feeding"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/layout_antenatal_profile">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/text_label_infant_feeding"
                        style="@style/Theme.LindaJamii.TextVisitHeading"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:text="Infant Feeding"
                        android:textAlignment="textStart"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_infant_feeding_couseling_done"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_label_infant_feeding">

                        <com.google.android.material.textfield.MaterialAutoCompleteTextView
                            android:id="@+id/text_infant_feeding_couseling"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Infant Feed Counseling Done "
                            android:inputType="text"
                            android:padding="10dp" />

                    </com.google.android.material.textfield.TextInputLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/text_layout_infant_feeding_couseling_breastfeeding"
                        style="@style/Theme.LindaJamii.TextInputLayout.Outlined"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/text_layout_infant_feeding_couseling_done">

                        <com.google.android.material.textfield.MaterialAutoCompleteTextView
                            android:id="@+id/text_infant_feeding_couseling_breastfeeding"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:hint="Exclusive Breastfeeding Couselling DOne "
                            android:inputType="text"
                            android:padding="10dp" />

                    </com.google.android.material.textfield.TextInputLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/button_finish_initial"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="8dp"
                    android:text="Finish Initial Visit"
                    app:icon="@drawable/ic_baseline_check_24"
                    app:layout_constraintEnd_toEndOf="@+id/layout_infant_feeding"
                    app:layout_constraintTop_toBottomOf="@+id/layout_infant_feeding" />


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.core.widget.NestedScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>


</layout>